# GitHub Token for fetching private repo data (Classic Token with 'repo' scope)
GITHUB_TOKEN=your_github_pat_here

# GitHub Username and Repository Name where the certifications.json is stored
# e.g. for github.com/username/my-portfolio
GITHUB_USERNAME=your_username
GITHUB_REPO=your_repo_name

# Path to the file in the repo (start with no slash)
# e.g. public/data/certifications.json
GITHUB_FILE_PATH=public/data/certifications.json
